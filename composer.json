{
    "name": "dsc/cms",
    "description": "A Laravel package that transforms a Laravel Inertia React app into a full WordPress-like CMS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "inertiajs/inertia-laravel": "^0.6",
        "spatie/laravel-permission": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dsc\\Cms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dsc\\Cms\\Providers\\CmsServiceProvider"
            ]
        }
    }
}